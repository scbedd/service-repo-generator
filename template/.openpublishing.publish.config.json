{
    "docsets_to_publish": [
      {
        "docset_name": "{{ config.docset_name }}",
        "build_source_folder": "{{ config.docset_name }}",
        "build_output_subfolder": "{{ config.docset_name }}",
        "locale": "en-us",
        "monikers": [],
        "moniker_ranges": [ {{ config.get_moniker_range() }} ],
        "open_to_public_contributors": false,
        "type_mapping": {
          "Conceptual": "Content",
          "ManagedReference": "Content",
          "RestApi": "Content",
          "UniversalReference": "Content",
          "JavaMember": "Content",
          "JavaType": "Content",
          "JavaPackage": "Content",
          "JavaEnum": "Content",
          "TSType": "Content",
          "TSPackage": "Content",
          "TSEnum": "Content",
          "TSTypeAlias": "Content",
          "ReferenceContainer": "Content"
        },
        "build_entry_point": "docs",
        "template_folder": "_themes",
        "base_path": "azure/developer",
        "customized_template_paths": [
            "_dependentPackages/memberpage.plugins/content"
        ],
        "customized_tasks": {
            "docset_prebuild": [
                "_dependentPackages/CommonPlugins/tools/SplitTOC.ps1",
                "_dependentPackages/CommonPlugins/tools/JoinTOC.ps1"
            ]
        },
        "site_name": "Docs"
      }
    ],
    "notification_subscribers": [
        "azuresdkdocreview@microsoft.com",
        "scbedd@microsoft.com"
    ],
    "sync_notification_subscribers": [],
    "branches_to_filter": [],
    "git_repository_branch_open_to_public_contributors": "master",
    "skip_source_output_uploading": false,
    "need_preview_pull_request": true,
    "contribution_branch_mappings": {},
    "dependent_repositories": [
      {
        "path_to_root": "_themes",
        "url": "https://github.com/Microsoft/templates.docs.msft",
        "branch": "master",
        "branch_mapping": {}
      }
    ],
    "branch_target_mapping": {},
    "need_generate_pdf_url_template": false,
    "dependent_packages": [
        {
          "id": "opbuild.templates.uref",
          "nuget_feed": "https://www.myget.org/F/op/api/v2",
          "path_to_root": "_dependentPackages/uref.plugins",
          "target_framework": "net45",
          "version": "latest"
        },
        {
          "id": "Microsoft.OpenPublishing.CommonPlugins",
          "nuget_feed": "https://www.myget.org/F/op/api/v2",
          "path_to_root": "_dependentPackages/CommonPlugins",
          "target_framework": "net45",
          "version": "latest"
        },
        {
          "path_to_root": "_dependentPackages/memberpage.plugins",
          "target_framework": "net45",
          "version": "latest",
          "id": "memberpage.plugins",
          "nuget_feed": "https://www.myget.org/F/docfx/api/v2"
        }
      ],
      "SplitTOC": [
        {% for moniker in config.monikers %}
        "{{ config.docset_name }}/{{ moniker }}/docs-ref-autogen/toc.yml"{{ "," if not loop.last }}
        {% endfor %}
      ],
      "JoinTOCPlugin": [
        {% for moniker in config.monikers %}
        {
            "TopLevelTOC": "docs-ref-mapping/reference-{{ moniker }}.yml",
            "ReferenceTOC": "{{ config.docset_name }}/{{ moniker }}/docs-ref-autogen/toc.yml",
            "ReferenceTOCUrl": "/{{ config.language }}/sdk/{{ config.service_label }}/toc.json",
            "OutputFolder": "{{ config.docset_name }}/{{ moniker }}/overview",
            "HideEmptyNode": false,
            "ContainerPageMetadata": {
                "langs": [
                "{{ config.lang }}"
                ],
                "open_to_public_contributors": false
            }
        }{{ "," if not loop.last }}
        {% endfor %}
    ],
    "docs_build_engine": {
      "name": "docfx_v3"
    }
}